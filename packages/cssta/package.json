{
  "name": "cssta",
  "version": "0.4.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "npm run clean:dist; npm run clean:vendor",
    "clean:dist": "rm -rf ./dist || :;",
    "clean:vendor": "rm -rf ./vendor || :;",
    "build": "npm run clean; npm run build:dist; npm run build:vendor",
    "build:dist": "babel ./src --out-dir ./dist --ignore __tests__,__mocks__",
    "build:vendor": "webpack --config ./webpack.config.js",
    "prepublish": "npm run build; cp ../../README.md ."
  },
  "keywords": [
    "postcss",
    "modules",
    "css-modules",
    "css",
    "minify",
    "min",
    "class",
    "className",
    "react",
    "css-in-js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jacobp100/cssta"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "css-color-function": "^1.3.0",
    "css-to-react-native": "^2.0.0",
    "dependency-graph": "^0.5.0",
    "events": "^1.1.1",
    "fbjs": "^0.8.6",
    "postcss": "^5.2.6",
    "postcss-selector-parser": "^2.2.2",
    "postcss-transform-animations": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^17.0.3",
    "react": "^15.4.1",
    "react-test-renderer": "^15.4.1",
    "webpack": "^1.14.0"
  },
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/web/index.js"
    ]
  }
}
